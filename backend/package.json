{
  "name": "@pietrobassi/bunnychess",
  "version": "0.1.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/gateway/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/gateway/test/jest-e2e.json",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathPattern=spec.ts",
    "test:integration": "jest --testPathPattern=integration",
    "test:coverage": "jest --coverage --coverageReporters=text --coverageReporters=lcov",
    "docker:attach": "docker compose up",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "start-all:dev": "concurrently \"yarn start:dev authentication\" \"yarn start:dev gateway\" \"yarn start:dev matchmaking\" \"yarn start:dev game\"",
    "start-all:debug": "concurrently \"yarn start:debug authentication\" \"yarn start:debug gateway\" \"yarn start:debug matchmaking\" \"yarn start:debug game\"",
    "generate-all": "yarn authentication:db:generate-schema && yarn matchmaking:db:generate-schema && yarn proto:generate",
    "authentication:db:generate-schema": "kysely-codegen --include-pattern=account --table-name-suffix=Schema --camel-case --url='env(POSTGRES_URL)' --env-file=apps/authentication/.env --out-file ./apps/authentication/src/database/schema.ts",
    "matchmaking:db:generate-schema": "kysely-codegen --include-pattern={ranking,mmr_change} --table-name-suffix=Schema --camel-case --url='env(POSTGRES_URL)' --env-file=apps/matchmaking/.env --out-file ./apps/matchmaking/src/database/schema.ts",
    "proto:generate": "protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=. --ts_proto_opt=nestJs=true --ts_proto_opt=useDate=true --ts_proto_opt=addGrpcMetadata=true --ts_proto_opt=addTimestampWrapper=false --ts_proto_opt=fileSuffix=.pb --experimental_allow_proto3_optional=true --ts_proto_opt=addNullToUndefined=true --ts_proto_opt=tsSuffix=Pb --ts_proto_opt=stringEnums=true --ts_proto_opt=unrecognizedEnum=false --ts_proto_opt=emitImportedFiles=false --ts_proto_opt=addServiceMethodsDecorator=false --ts_proto_opt=emptyToVoid=true $(find libs/common/src -iname '*.proto')"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.9.12",
    "@nestjs-modules/ioredis": "^1.0.1",
    "@nestjs/bullmq": "^11.0.1",
    "@nestjs/cache-manager": "^2.1.1",
    "@nestjs/cli": "^11.0.2",
    "@nestjs/common": "^11.0.5",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^11.0.5",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/microservices": "^11.0.5",
    "@nestjs/platform-express": "^11.0.5",
    "@nestjs/platform-socket.io": "^11.0.5",
    "@nestjs/terminus": "^10.2.0",
    "@nestjs/throttler": "^5.1.1",
    "@nestjs/websockets": "^11.0.5",
    "@pietrobassi/environment": "^0.1.1",
    "@pietrobassi/nestjs-nats-jetstream-transport": "^0.1.1",
    "@socket.io/redis-adapter": "^8.2.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.6",
    "@types/express": "^5.0.0",
    "@types/lodash": "^4.14.202",
    "@types/md5": "^2.3.5",
    "@types/ms": "^0.7.34",
    "@types/pg": "^8.10.9",
    "bcryptjs": "^2.4.3",
    "bullmq": "^4.15.2",
    "cache-manager": "^5.4.0",
    "cache-manager-redis-store": "^3.1.1",
    "chess.js": "^1.0.0-beta.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.7",
    "ioredis": "^5.3.2",
    "kysely": "^0.26.3",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "ms": "^2.1.3",
    "nats": "^2.18.0",
    "nest-winston": "^1.9.4",
    "nestjs-cls": "^5.0.0",
    "nestjs-grpc-reflection": "^0.2.2",
    "nestjs-throttler-storage-redis": "^0.4.1",
    "newrelic": "^11.7.0",
    "pg": "^8.11.3",
    "pg-error-enum": "^0.7.0",
    "redis": "^4.6.11",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.2",
    "ts-loader": "^9.4.3",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.5",
    "@pietrobassi/kysely-codegen": "^0.1.0",
    "@pietrobassi/ts-proto": "^0.1.2",
    "@types/jest": "^29.5.2",
    "@types/newrelic": "^9.14.3",
    "@types/node": "^22.10.9",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "jest-mock-extended": "^4.0.0-beta1",
    "prettier": "^3.0.0",
    "socket.io-client": "^4.7.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@common/(.*)$": "<rootDir>/libs/common/src/$1"
    }
  }
}